{
  "name": "crunevo",
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "dev:stack": "docker compose -f docker/docker-compose.yml up -d",
    "down:stack": "docker compose -f docker/docker-compose.yml down",
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "migrate": "turbo run migrate",
    "seed": "turbo run seed"
  },
  "devDependencies": {
    "turbo": "^1.10.12",
    "typescript": "^5.4.0"
  }
}
