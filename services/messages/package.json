{
  "name": "@crunevo/messages-service",
  "version": "0.1.0",
  "description": "CRUNEVO Messages Service - Real-time messaging and chat functionality",
  "main": "dist/main.js",
  "scripts": {
    "build": "npx prisma generate && tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "start:dev": "npx prisma generate && ts-node src/main.ts",
    "start:debug": "ts-node --inspect-brk src/main.ts",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev"
  },
  "keywords": ["nestjs", "messages", "chat", "websockets", "crunevo"],
  "author": "CRUNEVO Team",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/websockets": "^11.1.6",
    "@prisma/client": "^6.15.0",
    "prisma": "^6.15.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "jest": "^30.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
